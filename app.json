{
  "expo": {
    "name": "AnimeWorldz",
    "slug": "animeworldz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dovakiin0.animeworldz"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "Animeworldz now ported to mobile\n(Available for android only as of now)",
    "githubUrl": "https://github.com/Dovakiin0/animeworldz-mobile"
  }
}
